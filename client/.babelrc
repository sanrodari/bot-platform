{
  "env": {
    "test": {
      "presets": [
        "react",
        "es2015",
        "stage-1"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "react-hot-loader/babel"
      ]
    },
    "development": {
      "presets": [
        "react",
        [
          "es2015",
          {
            "modules": false
          }
        ],
        "stage-1"
      ],
      "plugins": [
        "transform-object-rest-spread",
        "react-hot-loader/babel"
      ]
    }
  }
}
